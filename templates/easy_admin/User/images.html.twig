<table class="table table-striped table-bordered">
    <thead>
    <tr>
        <th>#</th>
        <th>Image</th>
    </tr>
    </thead>
    {% for image in value %}
        <tr>
            <td>{{ image.id }}</td>
            <td><img src={{ asset(image.url )}}/></td>

        </tr>
    {% endfor %}
</table>